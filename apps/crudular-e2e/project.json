{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/crudular-e2e/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/cypress:cypress",
      "options": {
        "cypressConfig": "apps/crudular-e2e/cypress.json",
        "devServerTarget": "crudular:serve:development"
      },
      "configurations": {
        "production": {
          "devServerTarget": "crudular:serve:production"
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/crudular-e2e/**/*.{js,ts}"]
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional"
      }
    },
    "next-version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "dryRun": true
      }
    },
    "push-version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "preset": "conventional",
        "push": true
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["crudular"]
}
